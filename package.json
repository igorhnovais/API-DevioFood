{
    "name": "api-deviofood",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "directories": {
      "test": "tests"
    },
    "scripts": {
      "build": "npx tsc",
      "migration:run": "prisma migrate deploy",
      "start":" prisma migrate deploy && node dist/server.js",
      "dev": "nodemon src/server.ts",
      "test": "npm run test:load-envs jest  -- --passWithNoTests --runInBand",
      "test:coverage": "jest --coverage",
      "test:migration:run": "npm run test:load-envs prisma migrate deploy",
      "test:migration:generate": "npm run test:load-envs npx prisma migrate dev",
      "test:watch": "npm run test jest --watch --passWithNoTests --runInBand",
      "test:load-envs": "dotenv -e .env.test",
      "dev:load-envs": "dotenv -e .env.development",
      "dev:migration:run": "npm run dev:load-envs prisma migrate deploy",
      "dev:migration:generate": "npm run dev:load-envs prisma migrate dev",
      "test:report": "dotenv -e .env.test prisma migrate deploy & dotenv -e .env.test jest -- --coverage --runInBand --coverageReporters=json-summary --coverageDirectory=coverage --collectCoverageFrom='**/*.ts'"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
      "@faker-js/faker": "^8.3.1",
      "@prisma/client": "^4.9.0",
      "@types/node": "^20.10.1",
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "dotenv-expand": "^10.0.0",
      "express": "^4.18.2",
      "http-status": "^1.7.3",
      "joi": "^17.11.0",
      "pg": "^8.11.3",
      "typescript": "^5.3.2"
    },
    "devDependencies": {
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.21",
      "@types/jest": "^29.5.10",
      "@types/node": "^18.14.0",
      "@types/supertest": "^2.0.16",
      "dotenv-cli": "^7.0.0",
      "cross-env": "^7.0.3",
      "nodemon": "^3.0.1",
      "prisma": "^4.9.0",
      "supertest": "^6.3.3",
      "ts-node": "^10.9.1",
      "jest": "^29.7.0",
      "ts-jest": "^29.1.1"
    }
  }
  